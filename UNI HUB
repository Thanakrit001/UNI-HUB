local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "UNI HUB", HidePremium = false, SaveConfig = true, ConfigFolder = "UNI HUB"})

local SelectFarm 
local AutoFarmBarbell = false

local function EquipTool(toolName)
    local player = game:GetService("Players").LocalPlayer
    local tool = player.Backpack:FindFirstChild(toolName)

    if tool then
        player.Character.Humanoid:EquipTool(tool)
    else
        print("Tool not found in Backpack.")
    end
end

local function AutoFarm()
    while _G.AutoFarm do
        wait()
        pcall(function()
            local args = {
                [1] = "rep"
            }

            game:GetService("Players").LocalPlayer:WaitForChild("muscleEvent"):FireServer(unpack(args))

            EquipTool(SelectFarm)
        end)
    end
end
--
local function AutoFarmBarbellFunction()
    while AutoFarmBarbell do
        wait() 
        local player = game.Players.LocalPlayer
        local rootPart = player.Character and player.Character:FindFirstChild("HumanoidRootPart")
        
        if rootPart then
            rootPart.CFrame = CFrame.new(-2917.47314, 12.1008654, -178.445648)
            
            local interactSeat = workspace:WaitForChild("machinesFolder"):WaitForChild("Frost Lift"):WaitForChild("interactSeat")
            
            game:GetService("ReplicatedStorage").rEvents.machineInteractRemote:InvokeServer("useMachine", interactSeat)
            
            game.Players.LocalPlayer.muscleEvent:FireServer("rep", interactSeat)
        end
    end

    print("AutoFarm Barbell is now:", AutoFarmBarbell)
end

local Tab = Window:MakeTab({
    Name = "Main",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

local Section = Tab:AddSection({
    Name = "AutoFarm - Muscle Legends"
})

local BLACKPACK = {"Weight", "Situps", "Pushups", "Handstands"}

Tab:AddDropdown({
    Name = "Select Farm",
    Options = BLACKPACK,
    Default = "",
    Callback = function(Value)
        SelectFarm = Value
    end    
})

local AutoFarmToggle = {
    Name = "AutoFarm",
    Default = false,
    Callback = function(Value)
        _G.AutoFarm = Value
        print("AutoFarm is now:", Value)
        if Value then
            AutoFarm() 
        end
    end
}
Tab:AddToggle(AutoFarmToggle)

local Section = Tab:AddSection({
    Name = "AutoFarm Frost Gym (1 Rebirth)"
})

local AutoFarmBarbellToggle = {
    Name = "AutoFarm Barbell Size >10",
    Default = false,
    Callback = function(Value)
        AutoFarmBarbell = Value
        if Value then
            AutoFarmBarbellFunction()
        end
        print("AutoFarm Barbell is now:", Value)
    end    
}
Tab:AddToggle(AutoFarmBarbellToggle)
--

local Rebirth = Window:MakeTab({
    Name = "Rebirth",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

local rebirthSection = Rebirth:AddSection({
    Name = "Rebirth"
})

local AutoRebirthToggle = {
    Name = "AutoRebirth",
    Default = false,
    Callback = function(Value)
        _G.autoRebirthToggle = Value
        print("Auto Rebirth is now:", Value)
    end
}
rebirthSection:AddToggle(AutoRebirthToggle)
--

